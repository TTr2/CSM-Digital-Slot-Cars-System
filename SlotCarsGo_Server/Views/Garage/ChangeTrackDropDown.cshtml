@model SlotCarsGo_Server.Models.ViewModels.ChangeTrackViewModel
@using (Html.BeginForm("ChangeTrack", "Garage", FormMethod.Post, new { @class="form-horizontal", role = "form", enctype = "multipart/form-data" }))
{
    @Html.AntiForgeryToken()
    <div class="form-group">
<!--        Html.LabelFor(m => m.SelectedTrackId, new { class = "col-md-3 control-label" }) -->
        <div class="col-md-12">
            @Html.DropDownListFor(m => m.SelectedTrackId, Model.MyTracksListItems, "-- Select Track --", new { @class = "form-control"})
        </div>
        <div class="col-md-12">
            <input type="submit" class="btn btn-primary" value="Change Track" />
        </div>
    </div>
}
