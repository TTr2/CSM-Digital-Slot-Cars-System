@model SlotCarsGo_Server.Models.ViewModels.ChangeCarViewModel
@using (Html.BeginForm("ChangeCar", "Garage", FormMethod.Post, new { @class = "form-horizontal", role = "form", enctype = "multipart/form-data" }))
{
    @Html.AntiForgeryToken()
    <div class="form-group">
<!--        Html.LabelFor(m => m.SelectedCarId, new { class = "col-md-3 control-label" }) -->
        <div class="col-md-12">
            @Html.DropDownListFor(m => m.SelectedCarId, Model.CarsInGarageListItems, "-- Select Car --", new { @class = "form-control" })
        </div>
        <div class="col-md-12">
            <input type="submit" class="btn btn-primary" value="Change Car" />
        </div>
    </div>
}